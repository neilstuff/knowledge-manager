{
    "name": "knowledge-manager",
    "version": "1.0.0",
    "description": "Knowledge Manager application",
    "main": "main.js",
    "scripts": {
        "pack": "electron-builder --dir",
        "dist": "electron-builder --publish never",
        "start": "electron ."
    },
    "build": {
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                }
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thesourorange/knowledge-manager.git"
    },
    "keywords": [
        "Electron"
    ],
    "author": "Neil Brittliff",
    "license": "CC0-1.0",
    "dependencies": {
        "-": "0.0.1",
        "@fortawesome/fontawesome-free": "^5.15.3",
        "balloon-css": "^1.2.0",
        "jquery": "^3.4.1",
        "jszip": "^3.5.0",
        "mime": "^2.0.4"
    },
    "devDependencies": {
        "electron": "^15.1.1",
        "electron-builder": "^22.11.7",
        "node": "^16.10.0"
    },
    "bugs": {
        "url": "https://github.com/thesourorange/knowledge-manager/issues"
    },
    "homepage": "https://github.com/thesourorange/knowledge-manager#readme"
}
